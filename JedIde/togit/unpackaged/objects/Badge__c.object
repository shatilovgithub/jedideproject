    <webLinks>
        <fullName>Set_as_Image</fullName>
        <availability>online</availability>
        <description>Sets the first attachment image as the badge url</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Set as Image</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>//
// (c) Appirio 2013
//
// 9/12/2013 Kym Le  Original
//
// Queries the Attachment object based on the Badge and grabs the first attachment to // set as the image URL
//


{!REQUIRESCRIPT(&quot;/soap/ajax/28.0/connection.js&quot;)}


var imageAttachment = sforce.connection.query(&quot;SELECT Id FROM Attachment WHERE ParentId = &apos;{!Badge__c.Id}&apos; LIMIT 1&quot;);

var records = imageAttachment.getArray(&quot;records&quot;);

if (records[0] == null) {
  alert(&apos;Please attach an image for the Badge&apos;);
} else {

  var fileUrl = window.location.protocol + &apos;//&apos; + window.location.hostname + &apos;/servlet/servlet.FileDownload?file=&apos; + records[0].Id;

  var b = new sforce.SObject(&quot;Badge__c&quot;);
  b.Id = &quot;{!Badge__c.Id}&quot;;
  b.image_url__c = fileUrl;

  try {
    var result = sforce.connection.update([b]);

    if (!result[0].getBoolean(&quot;success&quot;)) {
      alert(&quot;An error occurred while trying to set image URL:\n\n&quot; + result);
    }
  } catch(e) {

    alert(e);
  }
}

window.location.reload();</url>
    </webLinks>
